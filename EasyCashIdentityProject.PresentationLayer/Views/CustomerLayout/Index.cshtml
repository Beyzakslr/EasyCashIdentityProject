

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_CustomerLayoutHeaderPartial")


<body class="nav-md footer_fixed">
    <div class="container body">
        <div class="main_container">

            @await Component.InvokeAsync("_CustomerLayoutSidebarPartial")


            @await Component.InvokeAsync("_CustomerLayoutNavbarPartial")

            <div class="right_col" role="main">
                <div class="">
                    <div class="page-title">
                        <div class="title_left">

                            <h2>@RenderBody()</h2>
                            <h3>Fixed Footer <small> Just add class <strong>footer_fixed</strong></small></h3>
                        </div>
                    </div>
                </div>
            </div>

            @await Component.InvokeAsync("_CustomerLayoutFooterPartial")
        </div>
    </div>
    @await Component.InvokeAsync("_CustomerLayoutSidebarPartial")

</body>
</html>

